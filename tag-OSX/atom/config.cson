"*":
  "activate-power-mode":
    plugins:
      playAudio: true
      screenShake: true
  "atom-beautify":
    general:
      _analyticsUserId: "3faef170-debe-4b47-85f0-3e6d8a50d255"
  "atom-material-ui":
    colors:
      abaseColor: "#ffa726"
      accentColor: "#3f8dff"
      genAccent: true
      predefinedColor: "Orange"
    fonts: {}
    tabs: {}
    treeView: {}
    ui:
      animations: false
      panelShadows: true
  "branch-status":
    personalAccessToken: "402dc84d5d65c812f68d399bf561da4102e61022"
  core:
    audioBeep: false
    closeEmptyWindows: false
    destroyEmptyPanes: false
    disabledPackages: [
      "vim-mode"
      "tool-bar-main"
      "tool-bar"
      "tool-bar-almighty"
      "ex-mode"
      "language-ansible"
      "linter-ansible-linting"
      "linter-python-pep8"
      "merge-conflicts"
      "metrics"
      "tree-view-git-branch"
      "project-manager"
      "git-control"
      "rails-i18n-autocomplete"
      "branch-status"
      "linter-codeclimate"
    ]
    excludeVcsIgnoredPaths: false
    ignoredNames: [
      ".git"
      "coverage"
      "tmp"
      "log"
    ]
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "rspec"
    ]
    telemetryConsent: "no"
    themes: [
      "one-dark-ui"
      "material-monokai-syntax"
    ]
  editor:
    invisibles: {}
    scrollPastEnd: true
    showIndentGuide: true
  "exception-reporting":
    userId: "a8aa2857-4dc9-6d77-a888-b88d08f37d30"
  "file-icons": {}
  "genesis-ui":
    distractionFree:
      hideBottom: false
      hideFiles: false
      hideTabs: false
  linter: {}
  "linter-ansible-linting":
    ANSIBLE0006: true
    ansibleLintExecutablePath: "/usr/local/bin/ansible-lint"
  "linter-codeclimate":
    disableTimeout: true
  "linter-haml": {}
  "linter-rubocop":
    command: "/usr/local/bin/rubocop"
  "linter-ui-default":
    panelHeight: 69
    showPanel: true
  minimap:
    absoluteMode: true
    displayMinimapOnLeft: true
    plugins:
      "git-diff": true
      "git-diffDecorationsZIndex": 0
  "project-viewer":
    customWidth: 132
    disclaimer:
      v120: false
  "python-tools": {}
  "rest-client":
    recent_requests_limit: 200
  rester: {}
  "ruby-block":
    highlightLine: false
    highlightLineNumber: true
    showBottomPanel: false
  "seti-syntax":
    themeColor: "Orange"
  "status-bar": {}
  "tool-bar":
    iconSize: "16px"
    visible: false
  "tree-view": {}
  "vim-mode":
    useClipboardAsDefaultRegister: true
    useSmartcaseForSearch: true
  welcome:
    showOnStartup: false
".git-commit.text":
  editor:
    preferredLineLength: 72
".rails.rjs.ruby.source":
  editor:
    preferredLineLength: 1000
".rails.ruby.source":
  editor:
    preferredLineLength: 100
".rester.source":
  editor:
    autoIndent: true
".ruby.source":
  editor:
    preferredLineLength: 100
